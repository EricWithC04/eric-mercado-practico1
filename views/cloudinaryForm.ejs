<%- include('partials/header') %>

<div
  class="vh-100 w-100 d-flex flex-column align-items-center justify-content-center"
>
  <form class="w-50 d-flex flex-column" id="formularioCloudinary" enctype="multipart/form-data">
    <div class="mb-3">
        <h2>En este formulario podras subir archivos mediante Cloudinary</h2>
    </div>
    <div class="mb-3">
      <label for="exampleFormControlTextarea1" class="form-label">Archivo (Cloudinary):</label>
      <input
        name="fileC"
        type="file"
      />
    </div>
    <input
      type="submit"
      value="Enviar"
      class="btn btn-primary align-self-center"
    />
    <div role="status" id="loader"></div>
    <p id="resultado"></p>
  </form>
</div>

<script src="/subir-archivos-cloudinary.js"></script>

<%- include('partials/footer') %>